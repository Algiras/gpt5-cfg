{
  "name": "cfg-chatgpt-interactions",
  "version": "1.0.0",
  "description": "Natural language tree editor using OpenAI GPT-5 Context Free Grammar constraints",
  "main": "dist/index.js",
  "license": "MIT",
  "keywords": [
    "chatgpt",
    "context-free-grammar",
    "xml",
    "typescript",
    "ai",
    "parser"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/cfg-chatgpt-interactions.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/cfg-chatgpt-interactions/issues"
  },
  "homepage": "https://github.com/your-username/cfg-chatgpt-interactions#readme",
  "scripts": {
    "build": "tsc && npm run copy-assets && npm run generate-lark",
    "generate-lark": "python3 scripts/generate_lark_parser.py",
    "copy-assets": "mkdir -p dist/grammar && cp src/grammar/*.lark dist/grammar/",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "author": "",
  "dependencies": {
    "blessed": "^0.1.81",
    "dotenv": "^16.3.1",
    "openai": "^4.53.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.23",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@3.6.4+sha512.e70835d4d6d62c07be76b3c1529cb640c7443f0fe434ef4b6478a5a399218cbaf1511b396b3c56eb03bc86424cff2320f6167ad2fde273aa0df6e60b7754029f"
}
